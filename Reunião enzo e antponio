def recomendar_sala(participantes, tipo_reuniao):
    if participantes <= 0:
        return "NÃºmero de participantes invÃ¡lido."
        
    if tipo_reuniao == "executiva" or participantes > 15:
        return "Sala Grande"
    elif participantes <= 5:
        return "Sala Pequena"
    elif participantes <= 15:
        return "Sala Media"
    #elif participantes >= 15:
        #return "Sala Grande"

participante =int(input("numero de participantes?"))
sala = recomendar_sala(participante,"exucutiva")
print(sala)
